@article{lambert_differential_2017,
	title = {Differential associations of threat and deprivation with emotion regulation and cognitive control in adolescence},
	volume = {29},
	issn = {0954-5794, 1469-2198},
	url = {https://www.cambridge.org/core/journals/development-and-psychopathology/article/differential-associations-of-threat-and-deprivation-with-emotion-regulation-and-cognitive-control-in-adolescence/C0E35C875B5A4279315399EB25DF3690},
	doi = {10.1017/S0954579416000584},
	abstract = {Research on childhood adversity has traditionally focused on single types of adversity, which is limited because of high co-occurrence, or on the total number of adverse experiences, which assumes that diverse experiences influence development similarly. Identifying dimensions of environmental experience that are common to multiple types of adversity may be a more effective strategy. We examined the unique associations of two such dimensions (threat and cognitive deprivation) with automatic emotion regulation and cognitive control using a multivariate approach that simultaneously examined both dimensions of adversity. Data were drawn from a community sample of adolescents (N = 287) with variability in exposure to violence, an indicator of threat, and poverty, which is associated with cognitive deprivation. Adolescents completed tasks measuring automatic emotion regulation and cognitive control in neutral and emotional contexts. Violence was associated with automatic emotion regulation deficits, but not cognitive control; poverty was associated with poor cognitive control, but not automatic emotion regulation. Both violence and poverty predicted poor inhibition in an emotional context. Utilizing an approach focused on either single types of adversity or cumulative risk obscured specificity in the associations of violence and poverty with emotional and cognitive outcomes. These findings suggest that different dimensions of childhood adversity have distinct influences on development and highlight the utility of a differentiated multivariate approach.},
	language = {en},
	number = {3},
	urldate = {2020-06-12},
	journal = {Development and Psychopathology},
	author = {Lambert, Hilary K. and King, Kevin M. and Monahan, Kathryn C. and McLaughlin, Katie A.},
	month = aug,
	year = {2017},
	note = {Publisher: Cambridge University Press},
	pages = {929--940},
	file = {Lambert et al_2017_Differential associations of threat and deprivation with emotion regulation and.pdf:C\:\\Users\\Peverill\\Zotero\\storage\\GK9E5ILX\\Lambert et al_2017_Differential associations of threat and deprivation with emotion regulation and.pdf:application/pdf;Snapshot:C\:\\Users\\Peverill\\Zotero\\storage\\NEV58INA\\C0E35C875B5A4279315399EB25DF3690.html:text/html},
