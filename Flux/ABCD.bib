	author = {Hodgson, Karen and Poldrack, Russell A and Curran, Joanne E and Knowles, Emma E and Mathias, Samuel and Göring, Harald HH and Yao, Nailin and Olvera, Rene L and Fox, Peter T and Almasy, Laura and Duggirala, Ravi and Barch, Deanna M and Blangero, John and Glahn, David C},
	month = dec,
