	month = dec,
	year = {2019},
	keywords = {Functional connectivity, Resting state fMRI, Reproducibility, Development, Cognitive ability, ABCD},
	pages = {100706},
	file = {Full Text:C\:\\Users\\Peverill\\Zotero\\storage\\YRUD6WM3\\Marek et al. - 2019 - Identifying reproducible individual differences in.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Peverill\\Zotero\\storage\\WYYVI4MP\\S1878929319302932.html:text/html},
