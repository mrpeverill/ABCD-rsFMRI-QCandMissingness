@article{thompson_structure_2019,
	title = {The structure of cognition in 9 and 10 year-old children and associations with problem behaviors: {Findings} from the {ABCD} study’s baseline neurocognitive battery},
	volume = {36},
	issn = {1878-9293},
	shorttitle = {The structure of cognition in 9 and 10 year-old children and associations with problem behaviors},
	url = {https://www.sciencedirect.com/science/article/pii/S1878929318301622},
	doi = {10.1016/j.dcn.2018.12.004},
	abstract = {The Adolescent Brain Cognitive Development (ABCD) study is poised to be the largest single-cohort long-term longitudinal study of neurodevelopment and child health in the United States. Baseline data on N= 4521 children aged 9–10 were released for public access on November 2, 2018. In this paper we performed principal component analyses of the neurocognitive assessments administered to the baseline sample. The neurocognitive battery included seven measures from the NIH Toolbox as well as five other tasks. We implemented a Bayesian Probabilistic Principal Components Analysis (BPPCA) model that incorporated nesting of subjects within families and within data collection sites. We extracted varimax-rotated component scores from a three-component model and associated these scores with parent-rated Child Behavior Checklist (CBCL) internalizing, externalizing, and stress reactivity. We found evidence for three broad components that encompass general cognitive ability, executive function, and learning/memory. These were significantly associated with CBCL scores in a differential manner but with small effect sizes. These findings set the stage for longitudinal analysis of neurocognitive and psychopathological data from the ABCD cohort as they age into the period of maximal adolescent risk-taking.},
	language = {en},
	urldate = {2023-02-20},
	journal = {Developmental Cognitive Neuroscience},
	author = {Thompson, Wesley K. and Barch, Deanna M. and Bjork, James M. and Gonzalez, Raul and Nagel, Bonnie J. and Nixon, Sara Jo and Luciana, Monica},
	month = apr,
	year = {2019},
	keywords = {Adolescence, Externalizing, Internalizing, Stress reactivity, Child behavior checklist, Neurocognition, NIH toolbox, Principal components analysis},
	pages = {100606},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Peverill\\Zotero\\storage\\BQSR5ISS\\Thompson et al. - 2019 - The structure of cognition in 9 and 10 year-old ch.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Peverill\\Zotero\\storage\\7L7KFBCF\\S1878929318301622.html:text/html},
}

