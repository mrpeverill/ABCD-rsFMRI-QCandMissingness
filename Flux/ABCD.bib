	month = jan,
	year = {2020},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Psychology, Cognitive neuroscience, Risk factors},
	pages = {91--97},
	file = {Marshall et al_2020_Association of lead-exposure risk and family income with childhood brain.pdf:C\:\\Users\\Peverill\\Zotero\\storage\\V2VBZK38\\Marshall et al_2020_Association of lead-exposure risk and family income with childhood brain.pdf:application/pdf;Snapshot:C\:\\Users\\Peverill\\Zotero\\storage\\XYNPJ32T\\s41591-019-0713-y.html:text/html},
