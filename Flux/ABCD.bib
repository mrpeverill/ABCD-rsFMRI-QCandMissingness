	pages = {101021},
	file = {Full Text:C\:\\Users\\Peverill\\Zotero\\storage\\VULCHYCY\\Gonzalez et al. - 2021 - An update on the assessment of culture and environ.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Peverill\\Zotero\\storage\\JQY36VJA\\S1878929321001110.html:text/html},
