@article{lichtin_material_2021,
	title = {Material hardship, prefrontal cortex–amygdala structure, and internalizing symptoms in children},
	volume = {63},
	issn = {0012-1630, 1098-2302},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/dev.22020},
	doi = {10.1002/dev.22020},
	abstract = {Material hardship, or difficulty affording basic resources such as food, housing, utilities, and health care, increases children's risk for internalizing problems. The uncinate fasciculus (UNC) and two of the gray matter regions it connects—the orbitofrontal cortex (OFC) and amygdala—may play important roles in the neural mechanisms underlying these associations. We investigated associations among material hardship, UNC microstructure, OFC and amygdala structure, and internalizing symptoms in children. Participants were 5–9-year-old children (N = 94, 61\% female) from socioeconomically diverse families. Parents completed questionnaires assessing material hardship and children's internalizing symptoms. High-resolution, T1-weighted magnetic resonance imaging (n = 51), and diffusion tensor imaging (n = 58) data were acquired. UNC fractional anisotropy (FA), medial OFC surface area, and amygdala gray matter volume were extracted. Greater material hardship was significantly associated with lower UNC FA, smaller amygdala volume, and higher internalizing symptoms in children, after controlling for age, sex, and family income-to-needs ratio. Lower UNC FA significantly mediated the association between material hardship and internalizing symptoms in girls but not boys. These findings are consistent with the notion that material hardship may lead to altered white matter microstructure and gray matter structure in neural networks critical to emotion processing and regulation.},
	language = {en},
	number = {2},
	urldate = {2022-02-07},
	journal = {Dev Psychobiol},
	author = {Lichtin, Rebecca D. and Merz, Emily C. and He, Xiaofu and Desai, Pooja M. and Simon, Katrina R. and Melvin, Samantha A. and Maskus, Elaine A. and Noble, Kimberly G.},
	month = mar,
	year = {2021},
	keywords = {amygdala, material hardship, depression, anxiety, white matter, uncinate fasciculus},
	pages = {364--377},
	file = {Lichtin et al_2021_Material hardship, prefrontal cortex–amygdala structure, and internalizing.pdf:C\:\\Users\\Peverill\\Zotero\\storage\\T98ACS2E\\Lichtin et al_2021_Material hardship, prefrontal cortex–amygdala structure, and internalizing.pdf:application/pdf;Snapshot:C\:\\Users\\Peverill\\Zotero\\storage\\S7GX8FVP\\dev.html:text/html},
